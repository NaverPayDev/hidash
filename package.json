{
    "name": "@naverpay/hidash",
    "version": "0.0.0",
    "type": "module",
    "description": "improved lodash",
    "exports": {
        "./isEmpty": {
            "import": {
                "types": "./dist/isEmpty.d.ts",
                "default": "./dist/isEmpty.js"
            }
        },
        "./size": {
            "import": {
                "types": "./dist/size.d.ts",
                "default": "./dist/size.js"
            }
        },
        "./keys": {
            "import": {
                "types": "./dist/keys.d.ts",
                "default": "./dist/keys.js"
            }
        },
        "./isNil": {
            "import": {
                "types": "./dist/isNil.d.ts",
                "default": "./dist/isNil.js"
            }
        },
        "./isArray": {
            "import": {
                "types": "./dist/isArray.d.ts",
                "default": "./dist/isArray.js"
            }
        },
        "./isObject": {
            "import": {
                "types": "./dist/isObject.d.ts",
                "default": "./dist/isObject.js"
            }
        },
        "./has": {
            "import": {
                "types": "./dist/has.d.ts",
                "default": "./dist/has.js"
            }
        }
    },
    "scripts": {
        "prepare": "lefthook install -f",
        "build": "pnpm run clean && vite build --config vite.config.ts",
        "release": "changeset publish",
        "clean": "rm -rf dist",
        "lint": "eslint '**/*.{js,jsx,ts,tsx}'",
        "lint:fix": "eslint '**/*.{js,jsx,ts,tsx}' --fix",
        "prettier": "prettier --check '**/*.{ts,tsx,js,mjs,cjs,jsx,json,yaml,yml}'",
        "prettier:fix": "prettier --write '**/*.{ts,tsx,js,mjs,cjs,jsx,json,yaml,yml}'",
        "md": "markdownlint '*.md'",
        "md:fix": "markdownlint --fix '*.md'",
        "test": "vitest run",
        "bench": "vitest bench --run",
        "changeset-version": "changeset version && pnpm run md:fix"
    },
    "author": "yc.effort@navercorp.com",
    "license": "MIT",
    "packageManager": "pnpm@9.12.3",
    "devDependencies": {
        "@babel/plugin-transform-runtime": "^7.25.9",
        "@changesets/cli": "^2.27.9",
        "@naverpay/browserslist-config": "^1.6.1",
        "@naverpay/editorconfig": "^0.0.4",
        "@naverpay/eslint-config": "^1.0.7",
        "@naverpay/markdown-lint": "^0.0.3",
        "@naverpay/prettier-config": "^1.0.0",
        "@rollup/plugin-babel": "^6.0.4",
        "@types/lodash": "^4.17.13",
        "@types/node": "^20.17.6",
        "browserslist-to-esbuild": "^2.1.1",
        "lefthook": "^1.8.2",
        "lodash": "^4.17.21",
        "rollup": "^4.24.3",
        "rollup-preserve-directives": "^1.1.2",
        "vite": "^5.4.10",
        "vite-plugin-dts": "^4.3.0",
        "vite-tsconfig-paths": "^5.0.1",
        "vitest": "^2.1.4"
    },
    "engines": {
        "node": ">=16.0.0"
    }
}
